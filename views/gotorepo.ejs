<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="gotorepo.css">
</head>
<body>
    <form action="/gotorepo" method="POST">
        <div id="dropinput">
            <input type="text" id="service" name="service" placeholder="Enter the service"><br>
            <div id="dropcontent">
                <% detail.services.forEach(function(item){%>
                    <button type="submit" name="service"  value=<%=item.s%>><%=item.s%></button>
                    <%}); %>
            </div>
        </div>
    </form>
</body>
</html>